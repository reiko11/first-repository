<div class="title">
    <h1>GeekInstagram</h1>
    <h3>新規Post</h3>
</div>
<div class="postform">
    <%= form_for @post do |f| %>
        <div class="field">
            <p><%= f.label :image %></p>
            <p><%= f.file_field :image %></p>
            <p><%= f.label :about %></p>
            <p><%= f.text_field :about %></p>
        </div>
        <%= f.submit "Post" %>
    <% end %>
    <%= link_to "投稿一覧に戻る", posts_path %>
</div>